{
  "name": "laborious",
  "version": "0.4.3",
  "description": "⚗️ CLI tools for the lazy Gitlab user",
  "license": "MIT",
  "repository": "reservix/laborious",
  "keywords": [
    "gitlab",
    "cli",
    "cli-app",
    "merge request",
    "tool"
  ],
  "author": "Reservix GmbH <karriere@reservix.net> (https://labs.reservix.de)",
  "contributors": [
    "Sebastian Sebald <sebastian.sebald@reservix.de>"
  ],
  "engines": {
    "node": ">=8"
  },
  "bin": {
    "laborious": "bin/index.js"
  },
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "LICENSE",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "chalk": "2.4.2",
    "commit-emojis": "1.0.0",
    "cosmiconfig": "5.0.7",
    "enquirer": "2.3.0",
    "enquirer-prompt-editor": "1.1.0",
    "execa": "1.0.0",
    "find-up": "3.0.0",
    "fs-extra": "7.0.1",
    "git-url-parse": "11.1.2",
    "got": "9.6.0",
    "indent-string": "3.2.0",
    "lodash.merge": "4.6.1",
    "log-symbols": "2.2.0",
    "opn": "5.4.0",
    "ora": "3.0.0",
    "yargs": "12.0.5",
    "yup": "0.26.10"
  },
  "devDependencies": {
    "@types/chalk": "2.2.0",
    "@types/cosmiconfig": "5.0.3",
    "@types/execa": "0.9.0",
    "@types/find-up": "2.1.1",
    "@types/fs-extra": "5.0.4",
    "@types/git-url-parse": "9.0.0",
    "@types/got": "9.4.0",
    "@types/indent-string": "3.2.0",
    "@types/jest": "23.3.13",
    "@types/lodash.merge": "4.6.4",
    "@types/log-symbols": "2.0.0",
    "@types/node": "10.12.18",
    "@types/opn": "5.1.0",
    "@types/ora": "3.0.0",
    "@types/tempy": "0.2.0",
    "@types/yargs": "12.0.8",
    "@types/yup": "0.26.6",
    "@typescript-eslint/eslint-plugin": "1.2.0",
    "conventional-changelog-cli": "2.0.11",
    "conventional-changelog-emojis": "1.3.0",
    "eslint": "5.13.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-xo": "0.26.0",
    "eslint-config-xo-typescript": "0.7.0",
    "eslint-plugin-jest": "22.2.2",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "prettier": "1.16.1",
    "pretty-quick": "1.10.0",
    "tempy": "0.2.1",
    "ts-jest": "23.10.5",
    "typescript": "3.2.4"
  },
  "scripts": {
    "start": "tsc",
    "test": "jest --config jest.config.js",
    "clean": "rm -rf lib",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "version": "conventional-changelog -p emojis -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  }
}
